{
  "expo": {
    "name": "my_snapchat",
    "slug": "my_snapchat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashsplashcreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFF00"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-image-picker",
        {
          "photosPermission": "My_Snapchat accesses your gallery to let you share images with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
